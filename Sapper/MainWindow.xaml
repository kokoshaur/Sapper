<Window x:Class="Sapper.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Sapper"
        mc:Ignorable="d"
        Title="MainWindow" Height="900" Width="816" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid>
        <Button x:Name="EzModButton" Content="Ez" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Click="EzModButton_Click"/>
        <Button x:Name="MidModButton" Content="Mid" HorizontalAlignment="Left" Margin="30,10,0,0" VerticalAlignment="Top" Click="MidModButton_Click"/>
        <Button x:Name="HardModButton" Content="Hard" HorizontalAlignment="Left" Margin="60,10,0,0" VerticalAlignment="Top" Click="HardModButton_Click"/>
        <WrapPanel x:Name="PanelField" Height="823" Width="816" Margin="0,61,0,0" MaxWidth="816" MaxHeight="823"/>

    </Grid>
</Window>
